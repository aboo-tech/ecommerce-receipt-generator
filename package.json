{
  "name": "ecommercereceipt",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "build": "tsc && copyfiles -u 1 src/**/*.hbs dist",
    "start": "npm run build && cross-env NODE_ENV=production node dist/app.js",
    "worker": "cross-env NODE_ENV=production node dist/utils/worker.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/app.ts",
    "worker:dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/utils/worker.ts"
  },
  "author": "aboo-tech",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^25.0.9",
    "@types/nodemailer": "^7.0.5",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bullmq": "^5.67.1",
    "cloudinary": "^2.9.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "handlebars": "^4.7.8",
    "ioredis": "^5.9.2",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.4",
    "nodemailer": "^7.0.12",
    "puppeteer": "^24.37.1"
  }
}
